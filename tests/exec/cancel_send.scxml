<scxml xmlns="http://www.w3.org/2005/07/scxml" initial="s0">
  <state id="s0">
    <onentry>
      <send event="tock" delay="1s" idlocation="sid"/>
    </onentry>
    <transition event="cancel" target="ok">
      <cancel sendidexpr="sid"/>
    </transition>
    <transition event="tock" target="fail"/>
  </state>
  <final id="ok"/>
  <final id="fail"/>
