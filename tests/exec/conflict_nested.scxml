<scxml xmlns="http://www.w3.org/2005/07/scxml" initial="A">
  <state id="A">
    <state id="B">
      <!-- Deeper transition should win on 'go' -->
      <transition event="go" target="C"/>
    </state>
    <!-- Parent transition with same trigger; should be shadowed by B's -->
    <transition event="go" target="C"/>
  </state>
  <state id="C"/>
</scxml>

